{
  "name": "bestofjs-batches",
  "version": "3.0.0",
  "description": "bestof.js.org batches (scheduled tasks)",
  "main": "index.js",
  "scripts": {
    "daily": "node batches github && node batches build && node batches hof && npm run deploy:prod",
    "batch-test": "node batches test",
    "build": "node batches build",
    "github": "node batches github",
    "deploy:prod": "firebase deploy --project prod",
    "deploy:dev": "firebase deploy --project dev",
    "test": "tape tests/**/*-spec.js",
    "test-debug": "tape tests/trends-spec.js",
    "test-topics": "node tests/get-topics.js",
    "lint": "standard ./batches/hof/index.js"
  },
  "author": "Michael Rambeau",
  "license": "MIT",
  "engines": {
    "node": ">=6.2.0"
  },
  "dependencies": {
    "async": "0.9.2",
    "bluebird": "^3.3.5",
    "dotenv": "1.2.0",
    "firebase-tools": "3.2.2",
    "fs-extra": "0.24.0",
    "lodash": "3.10.1",
    "minimist": "1.2.0",
    "moment": "2.10.6",
    "mongoose": "4.3.4",
    "node-fetch": "^1.5.3",
    "request": "2.63.0",
    "scrape-it": "^3.0.1",
    "winston-loggly": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.0",
    "faucet": "0.0.1",
    "standard": "^6.0.8",
    "tape": "4.2.0"
  }
}
