{
  "name": "bestofjs-batches",
  "version": "3.2.0",
  "description": "bestof.js.org batches (scheduled tasks)",
  "main": "index.js",
  "scripts": {
    "daily": "node batches github && node batches build && node batches hof && npm run deploy:prod && npm run notify",
    "batch-test": "node batches test",
    "build": "node batches build",
    "github": "node batches github",
    "deploy:prod": "firebase deploy --project prod",
    "deploy:dev": "firebase deploy --project dev",
    "notify": "node batches/send-daily-notification",
    "test": "tape tests/**/*-spec.js",
    "test-debug": "tape tests/trends-spec.js",
    "test-topics": "node tests/get-topics.js",
    "test-hof": "node tests/hof-spec.js",
    "lint": "standard ./batches/hof/index.js"
  },
  "author": "Michael Rambeau",
  "license": "MIT",
  "engines": {
    "node": ">=8.1.2"
  },
  "dependencies": {
    "async": "0.9.2",
    "bluebird": "^3.3.5",
    "dotenv": "1.2.0",
    "firebase-tools": "3.2.2",
    "fs-extra": "^4.0.1",
    "lodash": "3.10.1",
    "minimist": "1.2.0",
    "moment": "2.10.6",
    "mongoose": "4.3.4",
    "node-fetch": "^1.7.2",
    "package-json": "^4.0.1",
    "request": "2.63.0",
    "request-promise": "^4.2.1",
    "scrape-it": "^3.0.1",
    "winston-loggly": "^1.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.14.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^2.0.1",
    "prettier": "^1.5.3",
    "tape": "4.2.0"
  }
}
